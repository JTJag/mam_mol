$mol_gallery_demo $mol_example
	title \Gallery of cards
	count 101
	sub /
		<= App $mol_gallery
			items <= items /
	Item* $mol_link
		minimal_width 100
		uri \https://thiscatdoesnotexist.com/
		style *
			backgroundImage \url('https://thiscatdoesnotexist.com/')
		sub /
			<= Item_title* $mol_paragraph
				title <= item_title* \
	tags /
		\$mol_link
		\$mol_paragraph
		\gallery
		\image
		\adaptive
		\masonry
